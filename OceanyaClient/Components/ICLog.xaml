<UserControl x:Class="OceanyaClient.Components.ICLog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Width="232" Height="323">
    <ScrollViewer x:Name="ScrollViewer" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
        <RichTextBox x:Name="LogBox" IsReadOnly="True" VerticalAlignment="Stretch" VerticalScrollBarVisibility="Auto" BorderThickness="0" Background="{x:Null}" FontFamily="pack://application:,,,/Resources/Fonts/#Igiari Cyrillic" Foreground="White" FontSize="14"/>
    </ScrollViewer>
</UserControl>
